    <style>

        body {
            background-color: beige;
        }

        .parallax {
            min-height: 550px;
            background: transparent;
            display: flex;
            flex-direction: row;
        }

        .heading {
            z-index: 2;
            position: relative;
            margin-left: 40px;
            display: flex;
            flex: 0.5;
            flex-direction: column;
            justify-content: center;
            align-items: flex-start;
        }

        .items {
            flex: 0.5;
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: center;
            opacity: 0.8;
            top: 163px;
            z-index: 2;
            position: relative;
        }

        .circle>img {
            width: 100px;
            border: 4px solid white;
            border-radius: 70px;
            margin: 0px 4px;
            cursor: pointer;
        }

        /* features*/
        #features {
            padding: 9%;
            position: relative;
        }

        .feature-box {
            text-align: center;
            padding: 4%;
        }

        .icon {
            color: Black;
            margin-bottom: 1rem;
        }

        div>.fas {
            font-size: 4em;
        }

        .icon:hover {
            color: red;
        }

        .middle {
            min-height: 400px;
            background-color: #8fc549;
            /* background-color: blue; */
            display: flex;
        }

        .left {
            display: flex;
            flex-direction: column;
            flex: 0.3;
            justify-content: center;
            align-items: center;
        }

        .left>p {
            color: rgb(255, 240, 24);
        }

        .left>h2 {
            color: #fff;
        }

        .right {
            margin-top: 50px;
            flex: 0.7;
            display: flex;
            flex-direction: row;
        }

        .crops {
            width: 40px;
            margin: 6px;
        }

        .fas {
            font-size: 1.5em;
            margin: 8px;
        }

        .box {
            margin: 40px 4px;
        }

        .box>p {
            color: #ffff;
        }

        .container {
            height: 100vh;
            text-align: center;
            margin-top: 20px;
        }

        .col-md-5 {
            margin-top: 40px;
        }

        .col-md-6 {
            margin-top: 40px;
        }
